<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Midtrans Payment Provider Data -->
    <record id="payment_provider_midtrans" model="payment.provider">
        <field name="name">Midtrans</field>
        <field name="code">midtrans</field>
        <field name="state">disabled</field>
        <field name="is_published">False</field>
        <field name="sequence">50</field>
        <field name="image_128" type="base64" file="payment_midtrans/static/description/icon.png"/>
        <field name="display_as">Midtrans Payment Gateway</field>
        <field name="support_tokenization">False</field>
        <field name="support_manual_capture">False</field>
        <field name="support_refund">False</field>
        <field name="redirect_form_view_id" ref="midtrans_form"/>
    </record>
</odoo>